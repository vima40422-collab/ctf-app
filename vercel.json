{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "routes": [
    { "src": "/ifno/?", "dest": "/ifno/index.html" },
    { "src": "/scoreboard/?", "dest": "/scoreboard.html" },
    { "src": "/index.html", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}