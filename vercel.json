{
  "version": 2,
  "functions": {
    "api/*.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/scoreboard",
      "destination": "/scoreboard.html"
    },
    {
      "source": "/ifno",
      "destination": "/ifno.html"
    }
  ]
}